    "react-bootstrap": "^2.9.0",
    "react-bootstrap-icons": "^1.10.3",
    "react-dom": "^18.2.0",
    "react-markdown": "^9.0.0",
    "react-router": "^6.17.0",
    "react-router-dom": "^6.17.0",
    "remark-gfm": "^4.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not ie <= 11",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
