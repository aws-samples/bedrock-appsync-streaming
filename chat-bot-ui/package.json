{
  "name": "gen-ai-chatbot",
  "version": "2.0.0",
  "description": "Sample generative ai chat-bot - written in React 18+ with React Bootstrap and Native browser Fetch API.",
  "main": "index.js",
  "directories": {
    "test": "test",
    "src": "src"
  },
  "author": "AWS Professional Services",
  "repository": {
    "type": "git",
    "url": "https://github.com/aws-samples/bedrock-appsync-streaming"
  },
  "bugs": {
    "url": "https://github.com/aws-samples/bedrock-appsync-streaming/issues"
  },
  "homepage": "https://github.com/aws-samples/bedrock-appsync-streaming",
  "keywords": [
    "react",
    "bootstrap",
    "generative ai"
  ],
  "private": "true",
  "license": "MIT-0",
  "scripts": {
    "start-dev": "node ./node_modules/webpack/bin/webpack serve --config webpack.config.js --progress --stats-children --env NODE_ENV=development",
    "start-demo": "node ./node_modules/webpack/bin/webpack serve --config webpack.config.js --progress --stats-children --env NODE_ENV=development --env DEMO_MODE=true",
    "build": "webpack --env NODE_ENV=production --progress",
    "build-dev": "webpack --env NODE_ENV=development --progress",
    "build-demo": "webpack --env NODE_ENV=development --env DEMO_MODE=true --progress",
    "test-unit": "jest --coverage=true --testRegex='(/test/unit/.*\\.(test|spec))\\.js?$'",
    "test-unit-updatesnapshot": "jest --testRegex='(/test/unit/.*\\.(test|spec))\\.js?$' -u",
    "test-int": "jest --coverage=true --testRegex='(/test/integration/.*\\.(test|spec))\\.js?$'"
  },
  "devDependencies": {
    "@babel/core": "^7.23.2",
    "@babel/eslint-parser": "^7.22.15",
